--#include !/Cards/Card

MobWar = Card:new('Mob War', 'Start a Mob War.', 'Red', 1, false)

function MobWar:new()
    local o = {}
    setmetatable(o, self)
    self.__index = self
    return o
end

function MobWar:useScript(gameState, playerColor)
    HitList:startMobWar()
    return DECIDING_STATE
end
