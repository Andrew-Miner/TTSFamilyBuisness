--#include !/Cards/Card

Truce = Card:new('Truce',
                 'Stop a Mob War',
                 'Green', 1, false)

function Truce:new()
    local self = setmetatable({}, Truce)
    return self
end

function Truce:cardFunction(playerColor, closestColor)
    HitList:stopMobWar()
    return 'deciding'
end
